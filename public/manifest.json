{
  "manifest_version": 3,
  "name": "Enspell",
  "version": "0.0.1",
  "description": "DLsiteをもっと快適に楽しむためのツール",

  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },

  "permissions": ["storage"],

  "content_scripts": [
    {
      "matches": ["*://*.dlsite.com/*"],
      "js": ["circle_filter.js"],
      "run_at": "document_end"
    }
  ],

  "options_ui":{
    "page": "options/options.html",
    "open_in_tab": true
  }

}
